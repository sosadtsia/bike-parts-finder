{
  "name": "bike-parts-finder-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.0.0",
    "axios": "^1.6.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.20.0",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.3.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "docker:build": "docker build -t bike-parts-finder-frontend .",
    "docker:dev": "docker-compose up",
    "docker:prod": "docker build -t bike-parts-finder-frontend:prod . && docker run -p 80:80 bike-parts-finder-frontend:prod",
    "clean": "rm -rf node_modules .npm-cache",
    "reinstall": "npm run clean && npm ci",
    "cache:clear": "npm cache clean --force",
    "cache:verify": "npm cache verify"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "ajv": "^8.12.0",
    "ajv-keywords": "^5.1.0"
  }
}
